{
  "name": "utah",
  "version": "1.0.0",
  "description": "Utah Beachstorming Framework",
  "main": "dist/utah.js",
  "types": "dist/utah.d.ts",
  "repository": {
    "type": "git",
    "url": "git@github.com:ilanvolow/utah.git"
  },
  "files": [
    "/dist/**/*",
  ],
  "keywords": [
    "typescript",
    "framework",
    "utah",
    "jest",
    "express"
  ],
  "scripts": {
    "start": "nodemon --exec npm run buildRun",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "build-typecheck": "npm run build:types && npm run build:js",
    "buildRun": "npm run build && node dist/index.js",
    "build": "npm run clean:build && babel src --ignore src/__tests__ --out-dir dist --extensions '.ts,.tsx' --source-maps inline",
    "clean:build": "rm -rf dist",
    "build:types": "tsc --emitDeclarationOnly",
    "coverage": "jest --coverage",
    "prepare": "npm run build",
    "prepublish": "tsc",
    "prepublishOnly": "npm test",
    "test": "jest",
    "test:watch": "jest -o --watch",
    "typecheck": "tsc"
  },
  "homepage": "https://github.com/utah#readme",
  "bugs": {
    "url": "https://github.com/ilanvolow/utah/issues"
  },
  "author": "Elon Volo",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/node": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-numeric-separator": "^7.2.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "@babel/register": "^7.5.5",
    "@types/body-parser": "^1.17.0",
    "@types/cookie-parser": "^1.4.1",
    "@types/debug": "^4.1.4",
    "@types/express": "github:types/express",
    "@types/jest": "^24.0.16",
    "@types/morgan": "^1.7.36",
    "@types/node": "^10.5.1",
    "@types/webpack-env": "^1.14.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^24.9.0",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.7.0",
    "eslint": "^5.0.1",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.8.0",
    "eslint-plugin-standard": "^3.1.0",
    "jest": "^24.8.0",
    "nodemon-webpack-plugin": "^3.0.1",
    "supertest": "^4.0.2",
    "ts-jest": "^24.0.2",
    "ts-loader": "^4.5.0",
    "tslint": "^5.10.0",
    "typescript": "^3.5.3"
  },
  "dependencies": {
    "cookie-parser": "^1.4.4",
    "express": "^4.16.3",
    "morgan": "^1.9.1",
    "react": "^16.8.6",
    "tingodb": "^0.6.1"
  }
}
